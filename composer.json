{
    "name": "rubikalib/rubikalib",
    "description": "A library for working with rubika API from PHP source code",
    "version": "v2.0.0",
    "keywords": [
        "rubika",
        "rubikalib",
        "rubika-lib"
    ],
    "homepage": "https://RubikaLib-PHP.github.io",
    "readme": "https://github.com/AmirrezaFiroz/RubikaLib/blob/main/README",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RubikaLib\\": "bin/"
        }
    },
    "authors": [
        {
            "name": "amirreza",
            "email": "amirreza.firoz13@gmail.com",
            "homepage": "https://amirrezafiroz.github.io/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/AmirrezaFiroz/RubikaLib/issues"
    },
    "require": {
        "php": ">= 8.1.0",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ratchet/pawl": "^0.4.1",
        "james-heinrich/getid3": "^1.9",
        "phpseclib/phpseclib": "^3.0",
        "paragonie/sodium_compat": "^2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
